{
    "manifest_version": 3,
    "name": "Article Fetcher",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "scripting", "storage"],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["keys.js", "popup.js"] 
        }
      ]
    
}
